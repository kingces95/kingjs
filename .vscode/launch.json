    "version": "0.2.0",
    "configurations": [
    
    
      {
        "name": "Launch",
        "request": "launch",
        "type": "node",
        "program": "${file}",
        "cwd": "${fileDirname}",
        "console": "integratedTerminal",
        "skipFiles": ["<node_internals>/**/*.js"]
      },

      {
        "name": "Launch Pri",
        "request": "launch",
        "type": "node",
        "program": "${file}",
        "cwd": "${fileDirname}",
        "console": "integratedTerminal",
        "runtimeExecutable": "/Users/Setup/git/kingjs/node/out/Release/node"
      },

      {
        "name": "Attach",
        "request": "attach",
        "type": "node",
        "port": 36688,
      },

      {
        "name": "Test",
        "request": "launch",
        "type": "node",
        "program": "${fileDirname}/test.js",
        "cwd": "${fileDirname}",
        "console": "integratedTerminal",
      },

      {
        "name": "Exe",
        "request": "launch",
        "type": "node",
        "program": "${fileDirname}/exe.js",
        "cwd": "${workspaceFolder}/path",
        "console": "integratedTerminal",
      }      
    ]
}