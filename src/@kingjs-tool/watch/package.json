{
  "name": "@kingjs-tool/watch",
  "version": "1.0.0",
  "description": "A tool which, for each `package.json` found in any subdirectory, excluding dot directories, runs  `npm run build`  in the subdirectory whenever a change  is made to any file explicitly included in the package.",
  "main": "index.js",
  "files": [
    "*.js"
  ],
  "bin": {
    "wpkn": "./wsd.js"
  },
  "scripts": {
    "test": "node .test",
    "prepublishOnly": "npm test",
    "build": "cpk && crm && cdj && clk"
  },
  "repository": {
    "type": "git",
    "url": "https://repository.kingjs.net/watch-package-new"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "@kingjs/exec": "latest",
    "@kingjs/fs.rx.watch-many": "latest",
    "@kingjs/path.test": "latest",
    "@kingjs/rx.debounce": "latest",
    "@kingjs/rx.do": "latest",
    "@kingjs/rx.i-grouped-observable": "latest",
    "@kingjs/rx.i-observable": "latest",
    "@kingjs/rx.pool": "latest",
    "@kingjs/rx.select": "latest",
    "@kingjs/rx.select-many": "latest",
    "@kingjs/rx.skip": "latest",
    "@kingjs/rx.where": "latest",
    "@kingjs/rx.window-by": "latest",
    "@kingjs/shim": "latest",
    "minimatch": "latest"
  },
  "nodeDependencies": [
    "fs",
    "path"
  ]
}